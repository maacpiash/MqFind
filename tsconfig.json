{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "server/out",
    "declaration": false,
    "strict": true,
    "esModuleInterop": true,
    "noImplicitThis": false,
    "noImplicitAny": true,
    "strictPropertyInitialization": true
  },
  "include": [
    "server/src"
  ],
  "exclude": [
    "node_modules",
    "server/out"
  ]
}
